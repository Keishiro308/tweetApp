<%= provide :title, 'パスワードリセット'%>
<h1>パスワードリセット</h1>
<div class="row">
  <div class="col-md-6 offset-md-3">
    <%= form_with(scope: :password_reset, url: password_rests_path, local: true) do |form| %>
    <div class="form-group">
      <%= form.label :email %>
      <%= form.email_field :email, class: 'form-control' %>

    </div>
    <%= form.submit "Submit", class: "btn btn-primary btn-block" %>
    <% end %>
  </div>
</div>
